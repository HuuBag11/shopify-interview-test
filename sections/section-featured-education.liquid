{% schema %}
  {
    "name": "Featured Education",
    "tag": "section",
    "settings": [
      {
        "type": "text",
        "id": "sub_title",
        "label": "Sub Title"
      },
      {
        "type": "text",
        "id": "title",
        "label": "Title"
      },
      {
        "type": "richtext",
        "id": "desc",
        "label": "Description"
      }
    ],
    "blocks": [
      {
        "type": "Item",
        "name": "Person Item",
        "limit": 4,
        "settings": [
          {
            "type": "header",
            "content": "Content Person"
          },
          {
            "type": "image_picker",
            "id": "person_image",
            "label": "Image"
          },
          {
            "type": "text",
            "id": "person_title",
            "label": "Title"
          },
          {
            "type": "text",
            "id": "person_desc",
            "label": "Description"
          },
          {
            "type": "header",
            "content": "Social Links"
          },
          {
            "type": "url",
            "id": "fb_link",
            "label": "Facebook Link"
          },
          {
            "type": "url",
            "id": "ins_link",
            "label": "Instagram Link"
          },
          {
            "type": "url",
            "id": "tw_link",
            "label": "Twitter Link"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Featured Education"
      }
    ]
  }
{% endschema %}