<style>
  #featuredEducation .featuredEducation__list{
    list-style: none;
  }

  #featuredEducation .featuredEducation__wrapper{
    padding: 0;
  }

  #featuredEducation .subtitle{
    font-size: 14px;
    line-height: 24px;
    font-weight: 700;
    letter-spacing: 0.2px;
    color: var(--text-color-third);
    margin-bottom: 10px;
  }

  #featuredEducation .title{
    font-size: 24px;
    line-height: 32px;
    font-weight: 700;
    letter-spacing: 0.1px;
    color: var(--text-color-first);
    margin: 0;
    margin-bottom: 10px;
  }

  #featuredEducation .desc,
  #featuredEducation .desc *{
    font-size: 14px;
    line-height: 20px;
    color: var(--text-color-second);
    letter-spacing: 0.2px;
    font-weight: 400;
    margin-bottom: 0;
    font-family: var(--font-first);
  }

  #featuredEducation .featuredEducation__list{
    margin: 0 -15px;
    margin-top: 50px;
    list-style: none;
    row-gap: 30px;
  }

  #featuredEducation .featuredEducation__list--item{
    padding: 0 15px;
  }

  #featuredEducation .featuredEducation__list--item .item--wrapper{
    border-radius: 20px;
    overflow: hidden;
    box-shadow: 0 13px 19px 0 rgba(0, 0, 0, 0.09);
    transform: translateY(0);
    transition: all 0.5s ease;
    min-height: 100%;
  }

  #featuredEducation .featuredEducation__list--item .item--wrapper:hover{
    transform: translateY(-10px);
    box-shadow: 0 13px 19px 0 rgba(0, 0, 0, 0.2);
  }

  #featuredEducation .featuredEducation__list--item .item__image{
    width: 100%;
    aspect-ratio: 238 / 231;
    overflow: hidden;
  }

  #featuredEducation .featuredEducation__list--item .item__image img{
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  #featuredEducation .featuredEducation__list--item .item__content{
    padding: 30px;
    background-color: #FFF;
  }

  #featuredEducation .item__content--social{
    list-style: none;
  }

  #featuredEducation .featuredEducation__list--item .item__content--title{
    font-weight: 700;
    font-size: 16px;
    line-height: 24px;
    letter-spacing: 0.1px;
    color: var(--text-color-first);
    margin-bottom: 0;
  }

  #featuredEducation .featuredEducation__list--item .item__content--postion{
    font-size: 12px;
    line-height: 16px;
    letter-spacing: 0.2px;
    color: var(--text-color-second);
    margin-top: 10px;
  }

  #featuredEducation .featuredEducation__list--item .item__content--social{
    margin-top: 10px;
    gap: 20px;
  }

  #featuredEducation .featuredEducation__list--item .social--item svg{
    transition: all 0.4s ease;
    width: 100%;
    height: auto;
  }

  #featuredEducation .featuredEducation__list--item .social--item a:hover svg path{
    color: var(--text-color-first);
    fill: var(--text-color-first);
  }

  #featuredEducation .item__content--social .social--item{
    width: 25px;
    height: auto;
  }

  @media (max-width: 990px){
    #featuredEducation .featuredEducation__wrapper{
      padding: 0 30px;
      max-width: 100%;
    }
  }

  @media (max-width: 768px){
    #featuredEducation .title{
      font-size: 20px;
      line-height: 24px;
    }

    #featuredEducation .featuredEducation__list{
      margin: 0 -10px;
      margin-top: 30px;
      row-gap: 15px;
    }

    #featuredEducation .featuredEducation__list--item{
      padding: 0 7.5px;
    }

    #featuredEducation .featuredEducation__wrapper{
      padding: 0 20px;
    }

    #featuredEducation .featuredEducation__list--item .item__content{
      padding: 20px 10px;
    }

    #featuredEducation .featuredEducation__list--item .item__content--social{
      gap: 10px;
    }

    #featuredEducation .item__content--social .social--item{
      width: 20px;
      height: auto;
    }

    #featuredEducation .featuredEducation__list--item .item__content--title{
      line-height: 20px;
      font-size: 14px;
    }

    #featuredEducation .featuredEducation__list--item .item--wrapper:hover{
      transform: translate(0);
    }
  }
</style>

<section id="featuredEducation">
  <div class="featuredEducation__wrapper page-width page-width-desktop">
    {% if section.settings.sub_title != blank %}
      <span class="subtitle d-block">{{ section.settings.sub_title }}</span>
    {% endif %}

    {% if section.settings.title != blank %}
      <h1 class="title">{{ section.settings.title }}</h1>
    {% endif %}

    {% if section.settings.desc != blank %}
      <div class="desc">
        {{ section.settings.desc }}
      </div>
    {% endif %}

    {% if section.blocks != blank %}
    <ul class="featuredEducation__list row p-0 justify-content-center">
      {% for block in section.blocks %}
        <li class="featuredEducation__list--item col-lg-3 col-md-4 col-6">
          <div class="item--wrapper d-flex flex-column h-100">
            <div class="item__image">
              {% render 'image', image_desktop: block.settings.person_image, image_mobile: block.settings.person_image, image_alt: block.settings.person_title %}
            </div>
            <div class="item__content">
              {% if block.settings.person_title != blank %}
                <h3 class="item__content--title text-center">{{ block.settings.person_title }}</h3>
              {% endif %}

              {% if block.settings.person_desc != blank %}
                <span class="d-block item__content--postion text-center">{{ block.settings.person_desc }}</span>
              {% endif %}
              <ul class="item__content--social p-0 d-flex align-items-center justify-content-center">
